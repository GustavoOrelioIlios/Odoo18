<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_parking_payment_form_tree" model="ir.ui.view">
        <field name="name">parking.payment.form.tree</field>
        <field name="model">parking.payment.form</field>
        <field name="type">list</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
                <field name="company_id" groups="base.group_multi_company"/>
                <field name="active"/>
            </list>
        </field>
    </record>
    <record id="view_parking_payment_form_form" model="ir.ui.view">
        <field name="name">parking.payment.form.form</field>
        <field name="model">parking.payment.form</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                <group>
                    <field name="name"/>
                        <field name="company_id" groups="base.group_multi_company"/>
                        <field name="active"/>
                </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo> 