<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_move_form_sicoob_dados_pagamento" model="ir.ui.view">
        <field name="name">account.move.form.sicoob.dados.pagamento</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="base_payment_api.view_move_form_dados_pagamento_tab"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='dados_pagamento']/group" position="inside">
                <group string="Configurações Sicoob" name="sicoob_payment_config">
                    <field name="sicoob_nosso_numero" readonly="1"/>
                    <field name="sicoob_contract_number"/>
                    <field name="sicoob_payment_limit_date"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo> 