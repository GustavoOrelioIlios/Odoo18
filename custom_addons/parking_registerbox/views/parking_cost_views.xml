<odoo>
    <record id="view_parking_cost_tree" model="ir.ui.view">
        <field name="name">parking.cost.tree</field>
        <field name="model">parking.cost</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
                <field name="company_id"/>
                <field name="active"/>
                <field name="value"/>
            </list>
        </field>
    </record>
    <record id="view_parking_cost_form" model="ir.ui.view">
        <field name="name">parking.cost.form</field>
        <field name="model">parking.cost</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name"/>
                    <field name="company_id"/>
                    <field name="active"/>
                    <field name="value"/>
                </group>
            </form>
        </field>
    </record>
</odoo> 